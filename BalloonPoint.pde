class BalloonPoint
{
  String time;
  float latitude;
  float longitude;
  float altitude;
    
  BalloonPoint(String newTime, float newLatitude, float newLongitude, float newAltitude)
  {
    time = newTime;
    latitude = newLatitude;
    longitude = newLongitude;
    altitude = newAltitude;
  }
}
