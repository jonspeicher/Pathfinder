class BalloonPoint
{
  String time;
  String latitude;
  String longitude;
  String altitude;
    
  BalloonPoint(String newTime, String newLatitude, String newLongitude, String newAltitude)
  {
    time = newTime;
    latitude = newLatitude;
    longitude = newLongitude;
    altitude = newAltitude;
  }
}
